.wrapper{style: 'max-width: 1100px; margin: 0 auto; padding: 0 120px;'}
  - if @map_entry_json
    #map-entry-json{:'data-map-data' => @map_entry_json.to_json}
    %iframe{style: 'width: 100%; height: 430px', srcdoc: File.read('public/hub_map.html').gsub("// Allow custom map data for debugging", "window.__map_data = "+@map_entry_json) }
  - else
    %iframe{style: 'width: 100%; height: 430px', srcdoc: File.read('public/hub_map.html').gsub("https://sunrise-hub-json.s3.amazonaws.com/hubs.json", "/map/json")}
